{
  "name": "wykop-monitor",
  "version": "0.0.12",
  "description": "A package to monitor for new links, entries, mentions and private messages on Wykop.pl",
  "homepage": "https://github.com/xxdeepfriedxx/wykop-monitor",
  "license": "SEE LICENSE IN LICENSE",
  "main": "monitor.js",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "author": "xxdeepfriedxx",
  "dependencies": {
    "wykop": "^1.1.13"
  },
  "devDependencies": {
    "eslint": "^8.47.0"
  },
  "engines": {
    "node": ">=14.17.0"
  },
  "eslintConfig": {
    "env": {
      "commonjs": true,
      "es2021": true,
      "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": "latest"
    },
    "rules": {
      "indent": [
        "error",
        "tab"
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "always"
      ]
    }
  }
}
